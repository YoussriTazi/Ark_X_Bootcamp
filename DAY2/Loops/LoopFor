
//Type HelloWorld 5 Times using For Loop

for (let i = 0 ; i <=5; i++)
{
console.log("HelloWorld", i)
}



//Display Odd Numbers from 0 to 21

for (let i = 0 ; i<= 21; i++)
{
    if (i % 2 !==0) console.log(i)
}




//Display Even Numbers from 0 to 21
for (let i = 0 ; i<=21; i++)
{
    if (i % 2 === 0) console.log(i)
}




//Display Even Numbers from 0 to 21 in reverse
for (let i = 21 ; i >= 0; i--)
{
    if (i % 2 === 0) console.log(i)
}